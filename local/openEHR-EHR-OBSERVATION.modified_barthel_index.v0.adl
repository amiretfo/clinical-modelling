definition
	OBSERVATION[at0000] matches {    -- Barthel index
		data matches {
			HISTORY[at0002] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0003] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0001] matches {    -- Tree
								items cardinality matches {0..*; ordered} matches {
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Bowels
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0004] occurrences matches {0..1} matches {    -- Bladder
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Grooming
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {    -- Toilet use
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {    -- Feeding
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Transfer
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {    -- Mobility
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {    -- Dressing
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {    -- Stairs
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Bathing
										value matches {
											DV_QUANTITY matches {*}
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..100|}
											}
										}
									}
									ELEMENT[at0047] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0048] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0049] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0045] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

